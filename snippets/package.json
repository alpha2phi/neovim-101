{
  "name": "snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "languages": [
    {
      "id": "all",
      "extensions": "*"
    }
  ],
  "contributes": {
    "snippets": [
      {
        "language": [
          "all"
        ],
        "path": "./all.json"
      },
      {
        "language": [
          "lua"
        ],
        "path": "./lua.json"
      },
      {
        "language": [
          "markdown",
          "rmd",
          "rmarkdown"
        ],
        "path": "./markdown.json"
      },
      {
        "language": [
          "python"
        ],
        "path": "./python.json"
      }
    ]
  }
}
